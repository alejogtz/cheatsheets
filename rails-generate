# Generate rails job
rails generate job validate_gps_installation

# Generate schema migration
rails generate migration add_file_size_to_digital_signature

# Generate Controller in module Api::V1::Vigilante
rails generate controller Api::V1::Vigilante --no-template-engine

# Generate helper in specific module
rails generate helper Api::V1::ConverterHelper

# Generate Controller
bin/rails generate controller CreditCards open debit credit close

# Generate Mailer with action
rails generate mailer InsuranceRenewalMailer confirm_insurance

rails generate helper {{InsuranceRenewalProcessHelper}}
